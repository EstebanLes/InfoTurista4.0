        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InfoTurista40">
        <activity
            android:name=".adapter.adapterRecycler"
            android:exported="false" />
        <activity
            android:name=".model.activity_info"
